{
  "manifest_version": 3,
  "name": "AI Form Filler Pro",
  "description": "Fill your job applications with the click of a button, using the power of AI.",
  "version": "2.0",
  "permissions": [
    "activeTab",
    "scripting",
    "desktopCapture",
    "storage"
  ],
  "host_permissions": [
        "*://*/*"
    ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html"
}